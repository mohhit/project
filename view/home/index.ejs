
<div class="jumbotron" style="margin-top: -20px; height: 200px">

<h1 style="color:#B3B6B7;">WELLCOME TO E-SHOP</h1>
	<div style="background-image: url('images/1.jpg'); background-size: 100% 100%; height:200px; width: 100%;"></div>
</div>
<div id="myBox" class="contrainer-fluid" style="margin-top: 20px;">
	<div class="container" style="background-color: #B3B6B7;">
		<div class="row">
			<div class="col-sm-3">
				<ul class="list-group">
					<li class="list-group-item" style="background-color:#000;"><a style="color: #fff;" href="#">Category</a></li>

					<% catedata.forEach(function(z){ %>
            <li class="list-group-item" style="background-color:#000;"><a style="color: #fff;" href="#"><%= z.name; %></a></li>
       <% }); %>
					
					
				</ul>
			</div>
			<div class="col-sm-9">
				<ol class="breadcrumb" style="background-color: #000; color: #FFF !important;">
					<li>Popular Items</li>
				</ol>

				<div class="row">

                  <% prodata.forEach(function(z) { %>

			

						
					<div class="col-sm-4">
						<div class="panel panel-default">
							<div class="panel-heading">

								<h4><%= z.name; %> <small>(<%=z.price; %>)</small> </h4>
							</div>
<div class="panel-body">
	<img style="height: 150px; width: 100%;" src="/productimage/<%= z.image; %>" class="img img-thumbnail" />
</div>
							<div class="panel-footer">
								<button class="btn btn-primary btn-sm">Buy</button>
								<button class="btn btn-info btn-sm">Add To Cart</button>
							</div>
						</div>
					</div>

			
            <% }); %>

            </div>
			 
		</div>
	</div>
</div>

